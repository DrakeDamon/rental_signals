{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::${BUCKET}",
        "arn:aws:s3:::${BUCKET}/zillow/*",
        "arn:aws:s3:::${BUCKET}/aptlist/*",
        "arn:aws:s3:::${BUCKET}/fred/*"
      ]
    }
  ]
}