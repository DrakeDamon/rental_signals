# Copy to .env and fill in your values

# Snowflake configuration
SNOWFLAKE_ACCOUNT=<YOUR_SNOWFLAKE_ACCOUNT>
SNOWFLAKE_USER=<YOUR_SNOWFLAKE_USER>
SNOWFLAKE_PASSWORD=<YOUR_SNOWFLAKE_PASSWORD>
SNOWFLAKE_DATABASE=RENTS
SNOWFLAKE_WAREHOUSE=WH_XS
SNOWFLAKE_ROLE=ACCOUNTADMIN
SNOWFLAKE_SCHEMA=DBT_DEV

# API settings
API_TITLE=Tampa Rent Signals API
API_VERSION=1.0.0
API_DESCRIPTION=RESTful API for Tampa rental market data analysis and price tracking

# Query/performance
CACHE_TTL_SECONDS=3600
MAX_QUERY_LIMIT=100
DEFAULT_QUERY_LIMIT=20

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Environment
ENVIRONMENT=development
DEBUG=false

# Optional: Redis (if used)
# REDIS_URL=redis://localhost:6379/0
